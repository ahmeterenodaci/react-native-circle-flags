{
  "compilerOptions": {
    "rootDir": ".",
    "paths": {
      "react-native-circle-flags": ["./src/index"],
      "react-native-circle-flags/country": ["./src/country"],
      "react-native-circle-flags/language": ["./src/language"],
      "react-native-circle-flags/other": ["./src/other"],
      "react-native-circle-flags/country/*": ["./src/country/*/index"],
      "react-native-circle-flags/language/*": ["./src/language/*/index"],
      "react-native-circle-flags/other/*": ["./src/other/*/index"]
    },
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "customConditions": ["react-native-strict-api"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "verbatimModuleSyntax": true
  }
}
